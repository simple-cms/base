{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "dev-master#81614916082260c524b5a178e896f3f0ce79b820",
    "illuminate/html": "5.*",
    "simple-cms/core": "dev-master",
    "simple-cms/page": "dev-master",
    "simple-cms/blog": "dev-master",
    "simple-cms/tag": "dev-master",
    "simple-cms/sitemap": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "classmap": [
      "database",
      "tests/TestCase.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan route:scan",
      "php artisan event:scan",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}
